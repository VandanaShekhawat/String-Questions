public class MyClass {
    public static void main(String args[]) {
        //To find first non repeated character in a String
     String str ="ddllj";
     int len = str.length();
     char ch []= str.toCharArray();
     for(int i = 0; i<len;i++){
            boolean check = true;
         for(int j = 0;j<len;j++){
             if(i!=j && ch[i]== ch[j]){
                 check = false;
             }
         }
         if(check){
             System.out.println("First unique generated character is : " + ch[i]);
             break;
             }
        }
    }
}
